class parkingStall{
  int ypos, xpos;
  parkingStall(boolean occu, int x, int y){
    ypos = y;
    xpos = x;
    rect(xpos, ypos, 90,60);
    if(occu == true)
      fill(200,0,0);
    else
      fill(0,200,0);
  }   
}

class ParkingLotSection{
  void creatsection(){
    int j = 0;
    //parkingStall [] stalls = new parkingStall [24];
    for(int i = 0; i < 4; i++){
      stalls[i] = parkingStall(true,200,200);
    }
  }
}
